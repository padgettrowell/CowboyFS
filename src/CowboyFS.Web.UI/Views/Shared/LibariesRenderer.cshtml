@model IEnumerable<CowboyFS.Web.UI.Models.Library>

<ul data-role="listview">
    
    @{
        foreach (var library in Model)
        {
            <li><a href="@Url.Action("Index", new { library = library.LibraryId})"><img src="img/folders.png" alt="folder" class="ui-li-icon" />@library.DisplayName</a></li>
        }
    }
</ul>

